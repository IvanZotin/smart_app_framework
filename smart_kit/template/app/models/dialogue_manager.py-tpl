from smart_kit.models.dialogue_manager import DialogueManager


class CustomDialogueManager(DialogueManager):
    """В собственном DialogueManager можно переопределить логику, связанную с вызовом сценариев по входяшему интенту"""
    def __init__(self, scenario_descriptions, app_name, **kwargs):
        super().__init__(scenario_descriptions, app_name, **kwargs)